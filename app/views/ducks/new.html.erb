<h1><%= "Create New Duck" %></h1>

<%= form_for @duck do |f|%>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :description %>
    <%= f.text_field :description %>

    <br><%= f.label :student_id %>
    <%= f.collection_select :student_id, Student.all, :id, :name %>

    <br><%= f.submit %>
<% end %>